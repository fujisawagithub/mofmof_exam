<p id="notice"><%= notice %></p>

<h1>Rental Houses</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Fee</th>
      <th>Address</th>
      <th>Age</th>
      <th>Remarks</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rental_houses.each do |rental_house| %>
      <tr>
        <td><%= rental_house.name %></td>
        <td><%= rental_house.fee %></td>
        <td><%= rental_house.address %></td>
        <td><%= rental_house.age %></td>
        <td><%= rental_house.remarks %></td>
        <td><%= link_to 'Show', rental_house %></td>
        <td><%= link_to 'Edit', edit_rental_house_path(rental_house) %></td>
        <td><%= link_to 'Destroy', rental_house, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rental House', new_rental_house_path %>
